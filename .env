# New Relic License Key (REQUIRED)
NEW_RELIC_LICENSE_KEY=2a326cab47d49f5aab8db7ee009e4a57FFFFNRAL

# Endpoint for sending data (default: https://otlp.nr-data.net/v1/metrics)
# Use region-specific variants like https://otlp.eu01.nr-data.net/v1/metrics if needed
NEW_RELIC_OTLP_ENDPOINT=https://otlp.nr-data.net/v1/metrics

# Service name (default: otel-collector-host)
OTEL_SERVICE_NAME=otel-collector-host

# Environment (default: development)
OTEL_DEPLOYMENT_ENVIRONMENT=development

# Log level (default: info)
OTEL_LOG_LEVEL=info

# Metrics collection interval (default: 30s)
COLLECTION_INTERVAL=30s

# Batch send size (default: 1000)
BATCH_SEND_SIZE=1000

# Batch timeout (default: 10s)
BATCH_TIMEOUT=10s

# Memory limit in MiB (default: 250)
NEW_RELIC_MEMORY_LIMIT_MIB=250

# Memory spike limit in MiB (default: 150)
NEW_RELIC_MEMORY_SPIKE_LIMIT_MIB=150