# New Relic License Key - Required
NEW_RELIC_LICENSE_KEY=your_license_key_here

# Environment Settings
OTEL_DEPLOYMENT_ENVIRONMENT=production
OTEL_SERVICE_NAME=otel-collector-host

# Collection Intervals
COLLECTION_INTERVAL=30s

# Host Specific Variables - Useful for multi-environment deployment
# HOSTNAME=custom-host-name

# Log Level - Set to info for production, debug for troubleshooting
OTEL_LOG_LEVEL=info